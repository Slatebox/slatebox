{
  "name": "slatebox-app",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start": "concurrently \"testim connect\" \"stripe listen --forward-to http://localhost:3000/hooks\" \"meteor run --settings ./settings.dev.json\"",
    "startStaging": "concurrently \"testim connect\" \"stripe listen --forward-to http://localhost:3000/hooks\" \"meteor run --settings ./deploy/settings.staging.json\"",
    "startProd": "concurrently \"testim connect\" \"stripe listen --forward-to http://localhost:3000/hooks\" \"meteor run --settings ./deploy/settings.prod.json\"",
    "expModules": "NODE_OPTIONS='--experimental-modules'",
    "test": "meteor test --once --driver-package meteortesting:mocha",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "deploy": "mv ./node_modules ../node_modules_sb && cd ./.deploy && mup deploy && cd .. && mv ../node_modules_sb ./node_modules"
  },
