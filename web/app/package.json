{
  "name": "slatebox-web",
  "private": false,
  "sideEffects": false,
  "workspaces": {
    "nohoist": [
      "**"
    ]
  },
  "scripts": {
    "start": "meteor run --settings ./settings.dev.json",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "start-test": "cross-env NODE_OPTIONS=--max-old-space-size=8196 testim --webpackConfig='./webpack.config.js' run \"./tests/**/*.test.js\" --require-credentials",
    "test": "meteor npm run start-test",
    "dev-test": "cross-env NODE_OPTIONS=--max-old-space-size=8196 testim --webpackConfig='./webpack.config.js' run \"./tests/**/*.test.ts\" --require-credentials --reporters=chrome,console",
    "test-debug": "cross-env NODE_OPTIONS=--max-old-space-size=8196 testim --inspect 9229 --webpackConfig='./webpack.config.js' run \"./tests/**/*.test.ts\" --require-credentials",
    "test-debug-file": "cross-env NODE_OPTIONS=--max-old-space-size=8196 testim --inspect 9229 --webpackConfig='./webpack.config.js' --require-credentials run",
    "test-debug-examples": "cross-env NODE_OPTIONS=--max-old-space-size=8196 testim --inspect 9229 --webpackConfig='./webpack.config.js' run \"./tests/examples/*.test.ts\" --require-credentials",
    "test-headless": "cross-env NODE_OPTIONS=--max-old-space-size=8196 testim --inspect 9229 --webpackConfig='./webpack.config.js' run \"./tests/**/*.test.ts\" --require-credentials --headless",
    "test-ci": "cross-env NODE_OPTIONS=--max-old-space-size=8196 testim --webpackConfig='./webpack.config.js' run \"./tests/**/*.test.ts\" --grid testim-grid"
  },
